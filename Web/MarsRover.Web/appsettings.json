{
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:\\MarsRover\\Logs\\.log",
          "outputTemplate": "{Timestamp:HH:mm:ss} [{Level:w3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "Properties": {
      "Application": "Mars Rover"
    }
  },
  "AllowedHosts": "*",
  "MarsRover": {
    "Url": "https://api.nasa.gov/mars-photos/api/v1/",
    "Timeout": 10
  },
  "Auth": {
    "MarsRover": {
      "Key": "0VlIigF6s0iq5H4r01eYw4xh4134yzXS9ZFN1AL7"
    }
  },
  "ServiceBus": {
    "ConnectionString": "Endpoint=sb://marsrover-ns.servicebus.windows.net/;SharedAccessKeyName=send;SharedAccessKey=e2M9XyHXoCNQCh94Ey/lgZhaZv+kRC7ccZIq7y3jxZA=;EntityPath=imagesfordownload",
    "Queue": "imagesfordownload"
  }
}
